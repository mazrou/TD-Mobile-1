<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/noteListFragment">


    <fragment
        android:id="@+id/addNoteFragment"
        android:name="com.example.gtaches.addNoteFragment"
        android:label="fragment_add_note"
        tools:layout="@layout/fragment_add_note" >
        <action
            android:id="@+id/action_addNoteFragment_to_addDate"
            app:destination="@id/addDate" />
    </fragment>

    <fragment
        android:id="@+id/noteListFragment"
        android:name="com.example.gtaches.noteListFragment"
        android:label="fragment_note_list"
        tools:layout="@layout/fragment_note_list" >
        <action
            android:id="@+id/action_noteListFragment_to_addNoteFragment"
            app:destination="@id/addNoteFragment" />
    </fragment>
    <fragment
        android:id="@+id/addDate"
        android:name="com.example.gtaches.addDate"
        android:label="fragment_add_date"
        tools:layout="@layout/fragment_add_date" />
</navigation>